<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vbMedicineAttributes" xmlns:fx="http://javafx.com/fxml/1" fx:controller="medicines.medicine.MedicineController" spacing="10" alignment="TOP_CENTER">
    <ImageView fx:id="ivImage" fitHeight="200" fitWidth="200" />
    <Label fx:id="lbName" text="Medicine Name" />
    <Label fx:id="lbManufacturer" text="Manufacturer" />
    <Label fx:id="lbExpiryDate" text="Expiry Date" />
    <Label fx:id="lbQuantity" text="Quantity" />
    <Label fx:id="lbCount" text="Count" />
</VBox>
